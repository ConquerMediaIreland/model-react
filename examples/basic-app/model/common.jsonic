
sys: @"@voxgig/model/model/sys.jsonic"

main: ent: @'common-ent.jsonic'

main: shape: ent: {
  id: {
    field: 'id'
  }
  field: {
    id: kind: *'String' | string
  }
}

main: shape: ent: field: &: {
  kind: *'String' | string  
  label: string  
  inputType: *'text' | string
  required: *true | boolean  
  editable: *true | boolean
  options: &: {
    label: string
  }
} 